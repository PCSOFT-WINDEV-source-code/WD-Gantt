      //
      PROCEDURE SwitchTaskToStandard(LOCAL nRowSubscript is int)
      
      // Retrieves the parent task
      ATask is a GanttTask <- TABLE_Gantt.COL_Gantt[COL_TaskID[nRowSubscript]]
      
      // - switches the task to non cumulative
      ATask..Cumulative = False
      
      // - switches the task to non milestone
      ATask..Milestone = False
      
      // - positions the specific colors
      ATask..ProgressColor = COLOR_PROGRESS
      ATask..BackgroundColor = COLOR_BACKGROUND
      
      // - no image for the tasks
      TABLE_Gantt[nRowSubscript]..FolderImageWidth = 0
      
      // - restores the modification of columns 
      COL_StartDate[nRowSubscript]..State = Active
      COL_Duration[nRowSubscript]..State = Active
      COL_Progress[nRowSubscript]..State = Active
      
      // - fills the columns
      COL_Duration[nRowSubscript] = ATask..DurationInDay
      COL_Progress[nRowSubscript] = ATask..Progress
