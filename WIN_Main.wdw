            PROCEDURE ModifyProgress(gtModifiedTask is a GanttTask)
            
            // If the task is a cumulative task
            IF gtModifiedTask..Cumulative THEN
            	
            	// Restores the progress of the task
            	gtModifiedTask..Progress = gReferenceGanttTask..Progress
            	// Message to inform the user without locking him
            	ToastDisplay("Action not possible"+CR+"The cumulative tasks are automatically calculated: they cannot be modified", toastShort, vaMiddle, haCenter, PastelRed)
            	
            ELSE
            	
            	// Modifies the progress in the table
            	COL_Progress[gtModifiedTask..Row] = gtModifiedTask..Progress
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(gtModifiedTask..Row)
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            END
