      // Summary: Loads a wdgantt file
      // Syntax:
      //LoadFile (<sFile>)
      //
      // Parameters:
      // 	sFile: <specify the role of sFile>
      // Return value:
      // 	None
      //
      PROCEDURE LoadFile(sFile)
      
      nRowSubscript is int
      cATask is a GanttTask
      stAGanttInfo is STGanttChartInfo
      
      
      TABLE_Gantt..DisplayEnabled = False
      
      // Clears the structure
      VariableReset(gstGanttChart)
      // Clears the table
      GanttDeleteAll(COL_Gantt)
      TableDeleteAll(TABLE_Gantt)
      
      // Loads the file
      bufContent is Buffer = fLoadBuffer(sFile)
      
      // Deserializes the chart of tasks in XML format
      Deserialize(gstGanttChart, bufContent, psdXML)
      
      // Browses the array to add the tasks
      FOR ALL ELEMENT stAGanttInfo of gstGanttChart.arrTaskList
      	
      	IF stAGanttInfo.nParentSubscript = 0 THEN
      		// Adds the row
      		nRowSubscript = TableAddLine(TABLE_Gantt, stAGanttInfo.Task.ID, stAGanttInfo.Task.Titre, stAGanttInfo.Task.DateDébut, stAGanttInfo.Task.DuréeEnJour, stAGanttInfo.Task.Avancement, stAGanttInfo.Task)
      	ELSE
      		// Adds the row in child
      		nRowSubscript = TableAddChild(TABLE_Gantt, stAGanttInfo.nParentSubscript, stAGanttInfo.Task.ID, stAGanttInfo.Task.Titre, stAGanttInfo.Task.DateDébut, stAGanttInfo.Task.DuréeEnJour, stAGanttInfo.Task.Avancement, stAGanttInfo.Task)
      	END
      	
      END
      
      // Applies the "styles" to the table (image, grayed cell, etc.)
      FOR ALL ROW nSubscript of TABLE_Gantt
      	
      	// Retrieves the task
      	cATask <- COL_Gantt[COL_TaskID]
      	// If the task is a cumulative task
      	IF cATask..Cumulative THEN SwitchTaskToCumulative(nSubscript)
      	// If the task is a milestone
      	IF cATask..Milestone THEN SwitchTaskToMilestone(nSubscript)
      	// If the task is neither cumulative nor a milestone
      	IF cATask..Cumulative = False _AND_ cATask..Milestone = False THEN SwitchTaskToStandard(nSubscript)
      	
      END
      
      // Browses the array to add the links
      FOR ALL ELEMENT cAGanttLink of gstGanttChart.arrListLinks
      	GanttAddLink(COL_Gantt, cAGanttLink)
      END
      
      // Reorganizes the tasks automatically
      IF CBOX_AutomaticReorganization THEN GanttOrganizeTask(COL_Gantt)
      
      // Updates the information of the Gantt chart
      DisplayGeneralInfo()
      
      TABLE_Gantt..DisplayEnabled = True
      
      // Modifies the status of the buttons
      AdaptButtons()
     type : 458752
   -
     name : UpdateTasksAfterAutoReorganization
     procedure_id : 1760460094817738883
     type_code : 14
     code : |1+
      // Summary: Updates the information about the tasks in the table after an automatic reorganization
      // Syntax:
      //UpdateTasksAfterAutoReorganization ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE UpdateTasksAfterAutoReorganization()
      
      ATask is a GanttTask
      
      
      // Only for an automatic reorganization
      IF COL_Gantt..AutomaticReorganization = False THEN RETURN
      
      TABLE_Gantt..DisplayEnabled = False
      
      // Browses the table rows
      FOR ALL ROW nSubscript of TABLE_Gantt
      	
      	// Retrieves the task
      	ATask = COL_Gantt[COL_TaskID[nSubscript]]
      	
      	// Ignore the cumulative tasks (they will be calculated later)
      	IF ATask..Cumulative THEN CONTINUE
      	
      	// Updates the task information
      	COL_Progress[nSubscript] = ATask..Progress
      	COL_Duration[nSubscript] = ATask..DurationInDay
      	COL_StartDate[nSubscript] = ATask..StartDate
      	
      	// Recalculates the information about the parents
      	CalculateParentTasks(nSubscript)
      	
      END
      
      TABLE_Gantt..DisplayEnabled = True
     type : 458752
   -
     name : AdaptButtons
     procedure_id : 1760460094817804419
     type_code : 14
     code : |1-
      // Summary: Adapts the status of buttons to the chart content
      // Syntax:
      //AdaptButtons ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE AdaptButtons()
      
      // If the table is empty
      IF TABLE_Gantt..Count = 0 THEN
      	
      	// Grays the buttons used to create, save and print
      	BTN_New..State = Grayed
      	BTN_Save..State = Grayed
      	BTN_Print..State = Grayed
      	
      ELSE // If the table contains at least one row
      	
      	// Enables the buttons used to create, save and print
      	BTN_New..State = Active
      	BTN_Save..State = Active
      	BTN_Print..State = Active
      	
      END
