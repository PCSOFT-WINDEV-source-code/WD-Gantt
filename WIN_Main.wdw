            PROCEDURE EntryInInput(gtEdited is a GanttTask)
            
            // If a creation was in progress, don't switch to edit
            IF gbCreationAAF THEN
            	// The creation is no longer in progress
            	gbCreationAAF = False
            	// Selects the row
            	TableSelectPlus(TABLE_Gantt, gtEdited..Row)
            	// Cancels the edit
            	RESULT False
            END
            
            // Opens the window for task edit
            IF Open(WIN_Task, gtEdited) THEN
            	
            	// Updates the task information
            	COL_Title[gtEdited..Row] = gtEdited..Title
            	COL_StartDate[gtEdited..Row] = gtEdited..StartDate
            	COL_Duration[gtEdited..Row] = gtEdited..DurationInDay
            	COL_Progress[gtEdited..Row] = gtEdited..Progress
            	
            	// If the task is a cumulative task
            	IF gtEdited..Cumulative THEN SwitchTaskToCumulative(gtEdited..Row)
            	// If the task is a milestone
            	IF gtEdited..Milestone THEN SwitchTaskToMilestone(gtEdited..Row)
            	// If it is a simple task
            	IF gtEdited..Cumulative = False _AND_ gtEdited..Milestone = False THEN SwitchTaskToStandard(gtEdited..Row)
            	
            	// Reorganizes the tasks if necessary
            	IF COL_Gantt..AutomaticReorganization THEN GanttOrganizeTask(COL_Gantt)
            	
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(gtEdited..Row)
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            END
            
            RESULT False
