     name : UpdateChart
     procedure_id : 1760460094817607811
     type_code : 14
     code : |1-
      // Summary: Updates the chart
      // Syntax:
      //UpdateChart ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE UpdateChart()
      
      cTableTask is a GanttTask
      stBrowsedTask is STGanttChartInfo
      
      
      // Clears the variable
      VariableReset(gstGanttChart)
      
      // Stores the tasks
      FOR ALL ROW nSubscript of TABLE_Gantt
      	
      	// Retrieves the task for the table
      	cTableTask <- COL_Gantt[COL_TaskID[nSubscript]]
      	
      	// Clears the storage variable
      	VariableReset(stBrowsedTask)
      	// Stores the information
      	stBrowsedTask.sTaskID = cTableTask..ID
      	stBrowsedTask.nParentSubscript = TableGiveParent(TABLE_Gantt, nSubscript)
      	stBrowsedTask.Task = cTableTask
      	// Adds this task
      	Add(gstGanttChart.arrTaskList, stBrowsedTask)
      	
      END
      
      // Stores the links
      gstGanttChart.arrListLinks = GanttListLink(COL_Gantt)
