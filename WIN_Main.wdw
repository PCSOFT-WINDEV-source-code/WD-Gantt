           code : |1+
            PROCEDURE Moving(gtMoved is a GanttTask)
            
            // If the task is a cumulative task
            IF gtMoved..Cumulative THEN
            	
            	// Repositions the task
            	gtMoved..StartDate = gReferenceGanttTask..StartDate
            	gtMoved..Row = gReferenceGanttTask..Row
            	// Message to inform the user without locking him
            	ToastDisplay("Action not possible"+CR+"The cumulative tasks are automatically calculated: they cannot be modified", toastShort, vaMiddle, haCenter, PastelRed)
            	
            ELSE
            	
            	// If the row is different
            	IF gtMoved..Row <> gReferenceGanttTask..Row THEN
            		// Repositions the task
            		gtMoved..Row = gReferenceGanttTask..Row
            		// Message to inform the user without locking him
            		ToastDisplay("Caution"+CR+"You cannot move a task to another row", toastShort, vaMiddle, haCenter, PastelRed)
            	END
            	
            	// Modifies the beginning in the table
            	COL_StartDate[gtMoved..Row] = gtMoved..StartDate
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(gtMoved..Row)
            	// Updates all the tasks if necessary
            	UpdateTasksAfterAutoReorganization()
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            END
           type : 131
