           code : |1+
            PROCEDURE AfterLinkPrerequisiteTask (glLink is a GanttLink)
            
            cSourceTask is a GanttTask
            cDestinationTask is a GanttTask
            
            
            // Retrieves the tasks involved in the link
            cSourceTask <- GanttGetTask(COL_Gantt, glLink..SourceID)
            cDestinationTask <- GanttGetTask(COL_Gantt, glLink..DestinationID)
            
            // If one of the tasks is a cumulative task
            IF cDestinationTask..Cumulative _OR_ cSourceTask..Cumulative THEN
            	
            	// Deletes the link
            	GanttDeleteLink(COL_Gantt, glLink..SourceID, glLink..DestinationID)
            	// Repositions the destination task (that may have been modified)
            	cDestinationTask..StartDate = gReferenceGanttTask..StartDate
            	// Message to inform the user without locking him
            	ToastDisplay("Action not possible"+CR+"The cumulative tasks cannot be linked", toastShort, vaMiddle, haCenter, PastelRed)
            	
            ELSE
            	
            	// Modifies the start date in the table
            	COL_StartDate[cDestinationTask..Row] = cDestinationTask..StartDate
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(cDestinationTask..Row)
            	// Updates all the tasks if necessary
            	UpdateTasksAfterAutoReorganization()
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            END
           type : 226
