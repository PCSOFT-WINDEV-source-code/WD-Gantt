      // Summary: Displays the general project information: start, end, progress
      // Syntax:
      //DisplayGeneralInfo ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplayGeneralInfo()
      
      ATask is a GanttTask
      dStart is date = ""
      dEnd is date = ""
      nAdvance is int
      nElementNb is int
      
      
      // Browses the table
      FOR ALL ROW nSubscript of TABLE_Gantt
      	
      	// Only the first levels (cumulative tasks contain the necessary information)
      	IF TableGiveParent(TABLE_Gantt, nSubscript) > 0 THEN CONTINUE
      	
      	// Retrieves the task for this row
      	ATask <- COL_Gantt[COL_TaskID[nSubscript]]
      	
      	// Start date
      	dStart = (dStart = "" ? ATask..StartDate ELSE Min(dStart, ATask..StartDate))
      	
      	// End Date
      	dEnd = (dEnd = "" ? ATask..EndDate ELSE Min(dEnd, ATask..EndDate))
      	
      	// Progress
      	nAdvance += ATask..Progress
      	
      	// An additional element (do not count the milestones without progress)
      	IF ATask..Milestone = False THEN nElementNb++
      	
      END
      
      // Fills the caption
      IF TABLE_Gantt..Count = 0 THEN
      	STC_ProjectDetails = <§$0001§>
      ELSE
      	STC_ProjectDetails = StringBuild("Start date: %1 | End date: %2 | Global progress: %3 %", gFontBold(True)+DateToString(dStart, "DDDD DD MMMM YYYY")+gFontBold(False), gFontBold(True)+DateToString(dEnd, "DDDD DD MMMM YYYY")+gFontBold(False), gFontBold(True)+Round(nAdvance/Max(1, nElementNb)))	
      END
