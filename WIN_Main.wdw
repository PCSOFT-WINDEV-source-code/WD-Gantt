    name : BTN_Indent
    identifier : 0x286e69210b51a083
    internal_properties : 
    properties :
     x : 96
     y : 88
     width : 28
     height : 28
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        internal_properties : CAAAAAgAAAANgkADdfgm4wBo6Vv04PYldlhaaHerNJGdZoQOykXTaWzBcUnQT8UTql0SF3mY1UznQ7VLIKHGE2itnfKjUzMEy7e/Y2oZV2EXGGAhS5VsE7FeC0mbac9aywQt68FtHMYv40z2uDkpQOj6pjEM4Q9b8qkr7b+SEyg/nPEUt+GCgQ==
        code : |1+
         TaskBecomesParent is a GanttTask
         
         nSelectionSubscript is int = TableSelect(TABLE_Gantt)
         // A row must be selected
         IF nSelectionSubscript <= 0 THEN RETURN
         
         // Moves the current branch as child of the previous branch or on the same level (if the previous branch is in a lower level)
         nSubscriptSelectionParent is int = TableGiveParent(TABLE_Gantt, nSelectionSubscript)
         nSubscriptPreviousParent is int = TableGiveParent(TABLE_Gantt, nSelectionSubscript-1)
         nSubscriptNewParent is int
         nRowSubscript is int
         
         // If the two rows are already "sibling", switches the selection to child of the previous row
         IF nSubscriptSelectionParent = nSubscriptPreviousParent THEN
         	
         	// Retrieves the task that will become cumulative: inform if it is a milestone (and give the ability to cancel)
         	TaskBecomesParent = GanttGetTask(COL_Gantt,COL_TaskID[nSelectionSubscript-1])
         	IF TaskBecomesParent..Milestone THEN
         		IF YesNo(No, "Caution: the previous task of same level is a milestone and it will be transformed into cumulative task. Do you want to modify the hierarchy of the selected task?") = No THEN RETURN
         	END
         	
         	// Moves the branch as child of the previous branch
         	TableMoveBranch(TABLE_Gantt, nSelectionSubscript, nSelectionSubscript-1, tmInChild)
         	
         ELSE // If the two rows are not on the same level
         	
         	// Find the previous child on the same level 
         	nSubscriptPreviousChild is int
         	nhildSubscript is int = TableGiveChild(TABLE_Gantt, nSubscriptSelectionParent, taFirst)
         	WHILE nhildSubscript <> -1
         		
         		// If the browsed child is the selected one
         		IF nhildSubscript = nSelectionSubscript THEN
         			
         			IF nSubscriptPreviousChild = 0 THEN RETURN
         			
         			// We know the previous child found on the same level as the selection: list the children until the last one
         			nhildSubscript = TableGiveChild(TABLE_Gantt, nSubscriptPreviousChild, taFirst)
         			WHILE nhildSubscript <> -1
         				nRowSubscript = nhildSubscript
         				nhildSubscript = TableGiveChild(TABLE_Gantt, nSubscriptPreviousChild, taNext)
         			END
         			
         			// Moves the branch as sibling of the branch found
         			TableMoveBranch(TABLE_Gantt, nSelectionSubscript, nRowSubscript, tmInSibling)
         			
         			// End of process
         			BREAK
         			
         		END
         		
         		// Stores the child as previous
         		nSubscriptPreviousChild = nhildSubscript
         		
         		// Goes to the previous child
         		nhildSubscript = TableGiveChild(TABLE_Gantt, nSubscriptSelectionParent, taNext)
         		
         	END
         	
         END
         
         // Selects the row in its new position
         TableSelectPlus(TABLE_Gantt, nSelectionSubscript)
         
         // Stores the new parent of the selected branch
         nSubscriptNewParent = TableGiveParent(TABLE_Gantt, nSelectionSubscript)
         
         // Switches the parent task to cumulative
         SwitchTaskToCumulative(nSubscriptNewParent)
         
         // Updates the values of the former parent task
         CalculateParentTasks(nSubscriptSelectionParent)
         // Updates the values for the task
         CalculateParentTasks(nSelectionSubscript)
         // Updates the information of the Gantt chart
         DisplayGeneralInfo()
        type : 18
      -
        type : 161
        enabled : false
