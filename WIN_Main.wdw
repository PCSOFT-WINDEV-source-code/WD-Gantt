     code : |1+
      // Summary: Switches a task to cumulative
      // Syntax:
      //SwitchTaskToCumulative (<nRowSubscript> is int)
      //
      // Parameters:
      //	nRowSubscript (integer): Subscript of the row
      // Return value:
      // 	None
      //
      PROCEDURE SwitchTaskToCumulative(LOCAL nRowSubscript is int)
      
      // Retrieves the parent task
      ATask is a GanttTask <- TABLE_Gantt.COL_Gantt[COL_TaskID[nRowSubscript]]
      
      // - switches the task to cumulative
      ATask..Cumulative = True
      
      // - switches the task to non milestone
      ATask..Milestone = False
      
      // - positions the specific colors
      ATask..ProgressColor = (CBOX_ProgressCumulativeTask ? COLOR_PROGRESS ELSE COLOR_CUMULATIVE)
      ATask..BackgroundColor = (CBOX_ProgressCumulativeTask ? COLOR_BACKGROUND ELSE COLOR_CUMULATIVE)
      
      // - modifies the image of the row
      TABLE_Gantt[nRowSubscript]..ExpandedImage = IMG_Opened_directory
      TABLE_Gantt[nRowSubscript]..CollapsedImage = IMG_Closed_directory
      
      // - prevents from modifying the columns (except for the title)
      COL_StartDate[nRowSubscript]..State = Grayed
      COL_Duration[nRowSubscript]..State = Grayed
      COL_Progress[nRowSubscript]..State = Grayed
      
      // - fills the columns
      COL_Duration[nRowSubscript] = ATask..DurationInDay
      COL_Progress[nRowSubscript] = ATask..Progress
      
      // Defines the values for this task
      CalculateParentTasks(nRowSubscript)
