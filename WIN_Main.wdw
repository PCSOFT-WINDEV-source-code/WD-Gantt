     name : CalculateParentTasks
     internal_properties : CAAAAAgAAAAEvJnVT6NlWbK9yhgFjpHK/+L9A4QhfBdwkM5udecc+UoyfvYHXrYC7qx/RHiHmesUjW86J8mb/oO2MG7yrpA685cEybLh/wPIJ2cT9Zf50eiTdIm1iDXSXAkk0u0eoP0f1n3vRJcjXF7RDHcGGHciXRsvqiRtaP/mq6p4d69l2KtVhReUEXLPizjKSu+Tgha2yOlMp2MdxYgnHaFGaR1t/87+UsLf
     procedure_id : 1760460094817345667
     type_code : 14
     code : |1+
      // Summary: Calculates the information of parent tasks (start, duration, progress)
      // Syntax:
      //CalculateParentTasks (<nRowSubscript> is int)
      //
      // Parameters:
      //	nRowSubscript (integer): Subscript of the row
      // Return value:
      // 	None
      //
      PROCEDURE CalculateParentTasks(LOCAL nRowSubscript is int)
      
      // Subscript must be valid
      IF nRowSubscript <= 0 _OR_ nRowSubscript > TABLE_Gantt..Count THEN RETURN
      
      
      nParentSubscript is int
      cTask is a GanttTask
      cBrowsedTask is a GanttTask
      nDuration is int
      dtStart is datetime
      dtStartOfTask is datetime
      dtEnd is datetime
      dtEndOfTask is datetime
      nNbChildren is int
      nTotalProgress is int
      
      
      // Retrieves the task for the requested row
      cTask <- COL_Gantt[COL_TaskID[nRowSubscript]]
      // If the task is not a cumulative task
      IF cTask..Cumulative = False THEN
      	// We must position on the parent task
      	nParentSubscript = TableGiveParent(TABLE_Gantt, nRowSubscript)
      ELSE
      	// We must position on the task
      	nParentSubscript = nRowSubscript
      END
      
      // Moves up in the tree structure
      WHILE nParentSubscript > 0
      	
      	// Initializes the values
      	dtStart = ""
      	dtEnd = ""
      	nDuration = 0
      	nTotalProgress = 0
      	nNbChildren = 0
      	
      	// Browses the children of the row to perform the calculations
      	nhildSubscript is int = TableGiveChild(TABLE_Gantt, nParentSubscript, taFirst)
      	WHILE nhildSubscript <> -1
      		// Loads the task	
      		cBrowsedTask <- COL_Gantt[COL_TaskID[nhildSubscript]]
      		
      		// Increments the number of children (without counting the milestones)
      		IF cBrowsedTask..Milestone = False THEN nNbChildren++ 
      		
      		// Increments the progress total
      		nTotalProgress += Val(cBrowsedTask..Progress)
      		
      		// Stores the start and end of task
      		dtStartOfTask = cBrowsedTask..StartDate
      		dtEndOfTask = cBrowsedTask..EndDate
      		
      		// In order for the calculation of cumulative tasks to be correct, this end datetime is modified to the previous day at 18:00
      		IF cBrowsedTask..Milestone THEN
      			dtEndOfTask..Day--
      			dtEndOfTask..Time = "180000000"
      		END
      		
      		// Defines the start and the end (minimum date of subordinate tasks)
      		dtStart = (dtStart = "00000000000000000" ? dtStartOfTask ELSE Min(dtStart, dtStartOfTask))
      		dtEnd = (dtEnd = "00000000000000000" ? dtEndOfTask ELSE Max(dtEnd, dtEndOfTask))
      		
      		// Goes to the previous child
      		nhildSubscript = TableGiveChild(TABLE_Gantt, nParentSubscript, taNext)
      		
      	END
      	
      	// Loads the task	
      	cTask <- COL_Gantt[COL_TaskID[nParentSubscript]]
      	
      	// Applies the values
      	COL_Progress[nParentSubscript] = Round(nTotalProgress / Max(nNbChildren, 1))
      	cTask..Progress = COL_Progress[nParentSubscript]
      	COL_StartDate[nParentSubscript] = dtStart
      	cTask..StartDate = COL_StartDate[nParentSubscript]
      	
      	// The duration must be calculated by increment (step of 1) as long as the end date of the last task is not reached
      	// (therefore, the management of working days and bank holidays is automatically performed by the Gantt control)
      	cTask..DurationInDay = 1
      	WHILE cTask..EndDate < dtEnd..Date
      		cTask..DurationInDay++
      	END		
      	COL_Duration[nParentSubscript] = cTask..DurationInDay
      	
      	// We must position on the parent task
      	nParentSubscript = TableGiveParent(TABLE_Gantt, nParentSubscript)
      	
      END
      
      // Refreshes the display
      MultitaskRedraw(WIN_Main)
