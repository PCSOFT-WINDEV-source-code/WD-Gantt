     p_codes :
      -
        code : |1+
         nSelectionSubscript is int = TableSelect(TABLE_Gantt)
         
         // A row must be selected
         IF nSelectionSubscript <= 0 THEN RETURN
         
         // Move to the current level only
         arrLevelSubscript is array of int
         
         // Retrieves the parent subscript
         nParentSubscript is int = TableGiveParent(TABLE_Gantt, nSelectionSubscript)
         // Lists the children of this parent
         nChildNum is int
         nSubscriptPreviousChild is int
         nhildSubscript is int = TableGiveChild(TABLE_Gantt, nParentSubscript, taFirst)
         WHILE nhildSubscript <> -1
         	
         	// If the browsed child is the selected one
         	IF nhildSubscript = nSelectionSubscript THEN
         		
         		// If the selected branch becomes the first new child
         		IF nChildNum = 1 THEN
         			// Moves the row to the top
         			nhildSubscript = TableMoveBranch(TABLE_Gantt, nSelectionSubscript, nParentSubscript, tmInChild)
         		ELSE
         			IF nSubscriptPreviousChild <= 1 THEN RETURN
         			
         			// Moves the row to the top
         			nhildSubscript = TableMoveBranch(TABLE_Gantt, nSelectionSubscript, arrLevelSubscript[arrLevelSubscript..Count-1], tmInSibling)
         		END
         		
         		// Selects the row in its new position
         		TableSelectPlus(TABLE_Gantt, nhildSubscript)
         		
         		// End of process
         		BREAK
         		
         	END
         	// Stores the subscript
         	Add(arrLevelSubscript, nhildSubscript)
         	
         	// Stores the child as previous
         	nSubscriptPreviousChild = nhildSubscript
         	nChildNum++
         	// Goes to the previous child
         	nhildSubscript = TableGiveChild(TABLE_Gantt, nParentSubscript, taNext)
         	
         END
        type : 18
      -
        type : 161
        enabled : false
