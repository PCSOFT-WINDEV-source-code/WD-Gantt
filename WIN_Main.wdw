           code : |1-
            PROCEDURE BeforeCreation(gtCreated is a GanttTask)
            
            // Creation in progress
            gbCreationAAF = True
            
            // Default parameters of a task
            gtCreated..Progress = 0
            gtCreated..ProgressColor = COLOR_PROGRESS
            gtCreated..BackgroundColor = COLOR_BACKGROUND
            gtCreated..ID = GetIdentifier()
            gtCreated..Title = "New task "+gtCreated..ID
            
            // Opens the edit window
            IF Open(WIN_Task, gtCreated) THEN
            	
            	// Adds the new row	
            	let nSubscript = TableAddLine(TABLE_Gantt, gtCreated..ID, gtCreated..Title, gtCreated..StartDate, gtCreated..DurationInDay, gtCreated..Progress)
            	// Positions the created task in this row
            	gtCreated..Row = nSubscript
            	// Updates the task information
            	COL_Title[gtCreated..Row] = gtCreated..Title
            	COL_StartDate[gtCreated..Row] = gtCreated..StartDate
            	COL_Duration[gtCreated..Row] = gtCreated..DurationInDay
            	COL_Progress[gtCreated..Row] = gtCreated..Progress
            	
            	// If the task is a cumulative task
            	IF gtCreated..Cumulative THEN SwitchTaskToCumulative(gtCreated..Row)
            	// If the task is a milestone
            	IF gtCreated..Milestone THEN SwitchTaskToMilestone(gtCreated..Row)
            	// If it is a simple task
            	IF gtCreated..Cumulative = False _AND_ gtCreated..Milestone = False THEN SwitchTaskToStandard(gtCreated..Row)
            	
            	// Reorganizes the tasks if necessary
            	IF COL_Gantt..AutomaticReorganization THEN GanttOrganizeTask(COL_Gantt)
            	
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(gtCreated..Row)
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            ELSE
            	// Cancels the creation
            	RESULT False
            END
            
            RESULT True
