            PROCEDURE Resizing(gtResized is a GanttTask)
            
            // If the task is a cumulative task
            IF gtResized..Cumulative THEN 
            	
            	// Repositions the task
            	gtResized..StartDate = gReferenceGanttTask..StartDate
            	// Restores the duration of the task
            	gtResized..DurationInDay = gReferenceGanttTask..DurationInDay
            	// Message to inform the user without locking him
            	ToastDisplay("Action not possible"+CR+"The cumulative tasks are automatically calculated: they cannot be modified", toastShort, vaMiddle, haCenter, PastelRed)
            	
            ELSE
            	
            	// Modifies the duration and the beginning in the table
            	COL_Duration[gtResized..Row] = gtResized..DurationInDay
            	COL_StartDate[gtResized..Row] = gtResized..StartDate
            	// Starts calculating the parent cumulative tasks
            	CalculateParentTasks(gtResized..Row)
            	// Updates all the tasks if necessary
            	UpdateTasksAfterAutoReorganization()
            	// Updates the information of the Gantt chart
            	DisplayGeneralInfo()
            	
            END
           type : 132
         -
           internal_properties : CAAAAAgAAADzxeoRjY4K9o8MP/rkr9Dpo71svCgNQ1tPcpOAqLVv77G8Tlgz0HGNWEMjtCgGfV43mtrw4BTiNE6t2M2gMN50vUHEywXsWMgUnvLsFhI4Sls5iKP6K8Y3sBi9FNcBtySlRTfh4l/Gurwez5Q=
           code : |1-
            PROCEDURE Deletion(gtDeleted is a GanttTask)
            
            // Confirm the deletion
            IF YesNo(No, "Delete the task """+gtDeleted..Title+""" ?") = No THEN RESULT False
            
            // If it is a standard task
            IF gtDeleted..Cumulative = False THEN
            	
            	// Stores the subscript of the parent task
            	nParentSubscript is int = TableGiveParent(TABLE_Gantt, gtDeleted..Row)
            	
            	// Deletes the row
            	TableDelete(TABLE_Gantt, gtDeleted..Row)
            	
            	// If a parent task exists
            	IF nParentSubscript > 0 THEN
            		// If this task has no other child
            		IF TableGiveChild(TABLE_Gantt, nParentSubscript, taFirst) = -1 THEN
            			// The parent task becomes a standard task
            			SwitchTaskToStandard(nParentSubscript)
            		END
            	END
            	
            ELSE // If the task is a cumulative task
            	
            	arrSubscript is array of int
            	// Delete the indent of direct children
            	nhildSubscript is int = TableGiveChild(TABLE_Gantt, gtDeleted..Row, taFirst)
            	WHILE nhildSubscript <> -1
            		// Stores the row
            		Add(arrSubscript, nhildSubscript)	
            		// Goes to the previous child
            		nhildSubscript = TableGiveChild(TABLE_Gantt, gtDeleted..Row, taNext)
            	END
            	
            	// Browses the array in reverse order (so that the initial order is kept when moving rows)
            	FOR ALL ELEMENT nSubscript of arrSubscript FromEnd
            		nParentSubscript is int = TableGiveParent(TABLE_Gantt, nSubscript)
            		
            		// The row must have a parent
            		IF nParentSubscript > 0 THEN	
            			// Moves the current branch as child of the previous branch
            			TableMoveBranch(TABLE_Gantt, nSubscript, nParentSubscript, tmInSibling)
            		END
            		
            	END
            	
            	// Deletes the row
            	TableDelete(TABLE_Gantt, gtDeleted..Row)
            	
            END
            
            // Starts calculating the parent cumulative tasks (duration, beginning, progress)
            CalculateParentTasks(gtDeleted..Row)
            // Updates all the tasks if necessary
            UpdateTasksAfterAutoReorganization()
            // Updates the information of the Gantt chart
            DisplayGeneralInfo()
            
            // Modifies the status of the buttons
            AdaptButtons()
            
            // Deletes the task
            RESULT True
